<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2000/10/XMLSchema">
  <!-- XSchema deffinition for Address -->
  <element name="Duration">
    <complexType>
      <attribute name="duration" use="optional">
        <simpleType>
          <restriction base="string">
            <enumeration value="start_date"/>
            <enumeration value="end_date"/>
          </restriction>
        </simpleType>
      </attribute>
    </complexType>
  </element>
</schema>
